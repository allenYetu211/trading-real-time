# 数据库配置
DATABASE_URL=postgresql://username:password@localhost:5432/trading_engine
DB_HOST=localhost
DB_PORT=5432
DB_NAME=trading_engine
DB_USER=username
DB_PASSWORD=password

# 币安API配置
BINANCE_API_KEY=your_binance_api_key
BINANCE_SECRET_KEY=your_binance_secret_key
BINANCE_BASE_URL=https://api.binance.com

# Telegram Bot配置
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_AUTHORIZED_USERS=123456789,987654321

# 服务器配置
PORT=3000
NODE_ENV=development

# 日志配置
LOG_LEVEL=info

# 分析配置
ANALYSIS_INTERVAL_MINUTES=15
MAX_TRADING_PAIRS=20
RISK_REWARD_THRESHOLD=2.0

# 默认交易对
DEFAULT_SYMBOLS=BTCUSDT,ETHUSDT,BNBUSDT,ADAUSDT,SOLUSDT

# API限制配置
API_WEIGHT_LIMIT=1200
API_WEIGHT_BUFFER=200
API_REQUEST_TIMEOUT=5000

# WebSocket配置
WS_PORT=3001

# 缓存配置
REDIS_URL=redis://localhost:6379
CACHE_TTL_SECONDS=300